{
  "compilerOptions": {
    "lib": ["es2022"],
    "module": "commonjs",
    "target": "es2022",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
  
    "noImplicitAny": false,
    "removeComments": true,
    "outDir": "dist"
  },
  "include": ["ts/utils.ts", "ts/main.ts"]
}
